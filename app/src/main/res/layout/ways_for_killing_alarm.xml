<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#edbaba">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="295dp"
        android:layout_height="47dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="82dp"

        android:gravity="center"
        android:text="알람 종료 방법을 선택 하세요"
        android:textColor="#000"
        android:textSize="23dp"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#303f9e"
        android:gravity="center"
        android:text="알람 종료 방법"
        android:textColor="#f7f4f4"
        android:textSize="23dp"
        tools:ignore="RtlCompat" />

    <FrameLayout
        android:layout_width="137dp"
        android:layout_height="154dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="163dp"
        tools:ignore="RtlCompat">

        <LinearLayout
            android:id="@+id/basic_alarming_unselected"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:orientation="vertical"
            android:visibility="visible">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="123dp"
                app:srcCompat="@drawable/allarmpng_final" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:gravity="center_horizontal|bottom"
                android:text="기본 알람"
                android:textColor="#000"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/basic_alariming_selected"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:orientation="vertical"
            android:visibility="invisible">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="match_parent"
                android:layout_height="123dp"
                app:srcCompat="@drawable/allarmpng_final_white" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal|bottom"
                android:text="기본 알람"
                android:textColor="#f4eded"
                android:textSize="20dp" />
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="137dp"
        android:layout_height="156dp"

        android:layout_alignParentEnd="true"
        android:layout_marginEnd="18dp"
        android:layout_marginTop="163dp"
        tools:ignore="RtlCompat,UnknownId">

        <LinearLayout
            android:id="@+id/mapalamring_unselected"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:orientation="vertical"
            android:visibility="visible">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="123dp"
                app:srcCompat="@drawable/mappng" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal|bottom"
                android:text="위치 찍기"
                android:textColor="#000"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mapalaming_selected"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:orientation="vertical"
            android:visibility="invisible">

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="match_parent"
                android:layout_height="123dp"
                app:srcCompat="@drawable/mappng_white" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal|bottom"
                android:text="위치 찍기"
                android:textColor="#f4eded"
                android:textSize="20dp" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="137dp"
        android:layout_height="156dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="18dp"
        android:layout_marginBottom="82dp"
        tools:ignore="RtlCompat">

        <LinearLayout
            android:id="@+id/math_solving_unselected"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:orientation="vertical"
            android:visibility="visible">

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="match_parent"
                android:layout_height="123dp"
                app:srcCompat="@drawable/mathblack" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="bottom|center_horizontal"
                android:text="수학풀이"
                android:textColor="#000"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/math_solving_selected"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:orientation="vertical"
            android:visibility="invisible">

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="match_parent"
                android:layout_height="123dp"
                app:srcCompat="@drawable/math_white" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="bottom|center_horizontal"
                android:text="수학풀이"
                android:textColor="#f4eded"
                android:textSize="20dp" />

        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="137dp"
        android:layout_height="156dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="380dp"
        android:layout_marginRight="18dp"
        tools:ignore="RtlCompat">

        <LinearLayout
            android:id="@+id/take_photo_unselected"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:orientation="vertical"
            android:visibility="visible">

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="match_parent"
                android:layout_height="123dp"
                app:srcCompat="@drawable/black_barcode" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="bottom|center_horizontal"
                android:text="바코드찍기"
                android:textColor="#000"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/take_photo_selected"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:orientation="vertical"
            android:visibility="invisible">

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="match_parent"
                android:layout_height="123dp"
                app:srcCompat="@drawable/white_barcode" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="bottom|center_horizontal"
                android:text="바코드찍기"
                android:textColor="#f4eded"
                android:textSize="20dp" />
        </LinearLayout>
    </FrameLayout>

</RelativeLayout>